<h3>登録済みルーム一覧</h3>
 
<table>
  <thead>
    <tr>
      <th>ルーム画像</th>
      <th>ルーム名</th>
      <th>ルーム紹介</th>
      <th>料金</th>
      <th>作成日</th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= image_tag room.room_image %></td>
        <td><%= room.room_name %></td>
        <td><%= room.room_introduction %></td>
        <td><%= room.price %></td>
        <td><%= room.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
